nav([ngClass]='{scrollnav: background, opacity: opacity}', (window:scroll)='changeBg()')
  .wrapper
    img.logo(routerLink='/home', src='data:image/png;base64,')
    .menu
      a(routerLink='/home', routerLinkActive='active', (click)='clearHeader()', translate key="nav.home") Kezdőlap
      a(routerLink='/gallery', routerLinkActive='active', (click)='clearHeader()', translate key="nav.gallery") Galéria
      a(routerLink='/events', routerLinkActive='active', (click)='clearHeader()', translate key="nav.events") Élmények
      a(routerLink='/contact', routerLinkActive='active', (click)='clearHeader()', translate key="nav.contacts") Kapcsolat
      a(routerLink='/delete-booking', routerLinkActive='active', (click)='clearHeader()', translate key="nav.cancel") Lemondás
      a.booking(routerLink='/booking', (click)='clearHeader()', translate key="nav.booking") Foglalás
      .language-selector((click)='langSelector=!langSelector', [ngClass]="'lng-' + getLanguageClass()")
        .language-list(*ngIf='langSelector', [@inOutAnimation])
          .lang-item.lng-hu((click)='selectLanguage("hu")')
          .lang-item.lng-en((click)='selectLanguage("en")')
      .language-list-hamburger
        .lang-item.lng-hu((click)='selectLanguage("hu")')
        .lang-item.lng-en((click)='selectLanguage("en")')
    .container((click)="animation=!animation; changeBg(); hamburger=!hamburger")
      .bar1([ngClass]="animation ? 'cbar1' : 'bar1'")
      .bar2([ngClass]="animation ? 'cbar2' : 'bar2'")
      .bar3([ngClass]="animation ? 'cbar3' : 'bar3'")
